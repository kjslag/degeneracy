      SubsuperscriptBox["Z", "0", "2"]}], "+", 
     RowBox[{
      SubscriptBox["X", 
       RowBox[{"-", "4"}]], 
      SubsuperscriptBox["Y", 
       RowBox[{"2", ",", "9", ",", "2"}], 
       RowBox[{"-", "1"}]]}], "+", 
     RowBox[{"h", ".", "c", "."}]}]}], TraditionalForm]],ExpressionUUID->
  "51b8f6c8-4af2-4909-9e71-83bba84d8c38"]
