 CellChangeTimes->{{3.830362280394409*^9, 
  3.830362284016881*^9}},ExpressionUUID->"2b1c81d1-5681-48a6-83e5-\
171b4779c8a3"],

Cell[TextData[{
 "Calculate the GSD of the string-membrane-net fracton model defined in \
Appendix A of https://arxiv.org/abs/1812.01613\nBelow, we use parameters ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", ",", 
    SubscriptBox["M", "k"], ",", 
    SubscriptBox["n", "k"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7c26a354-88f7-4828-af98-545fa8835728"],
 " to reproduce the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["Z", "3"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0cd003d4-cb32-45fb-b5e8-fb8cc3a729c0"],
 " X-cube model example above."
}], "Text",
 CellChangeTimes->{{3.830362582788702*^9, 3.830362675615155*^9}, {
  3.8303637898296423`*^9, 
  3.830363817512833*^9}},ExpressionUUID->"93173067-2afe-4ce8-bf6a-\
4394e9ea272e"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalNu]", "=", "3"}], ";", 
   RowBox[{"M", "=", 
    RowBox[{"{", 
     RowBox[{"\[CapitalNu]", ",", "\[CapitalNu]", ",", "\[CapitalNu]"}], 
     "}"}]}], ";", 
   RowBox[{"n", "=", 
    RowBox[{"{", 
     RowBox[{"1", ",", "1", ",", "1"}], "}"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"p", "=", 
   RowBox[{"LCM", "[", 
    RowBox[{"\[CapitalNu]", ",", 
     RowBox[{"Sequence", "@@", "M"}]}], "]"}]}], ";", 
  RowBox[{"m", "=", 
   RowBox[{"n", " ", 
    RowBox[{"M", "/", "\[CapitalNu]"}]}]}], ";", 
  RowBox[{"nK", "=", 
   RowBox[{"Length", "@", "n"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ops", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"b", ",", "c", ",", "\[Epsilon]"}], "}"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"b", ",", "c"}], "}"}], "=", 
           RowBox[{"DeleteCases", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"1", ",", "2", ",", "3"}], "}"}], ",", "a"}], "]"}]}], 
          ";", "\[IndentingNewLine]", 
          RowBox[{"\[Epsilon]", "=", 
           RowBox[{
            RowBox[{"LeviCivitaTensor", "[", "3", "]"}], 
            "\[LeftDoubleBracket]", 
            RowBox[{"a", ",", "b", ",", "c"}], "\[RightDoubleBracket]"}]}], 
          ";", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"a", ">", "nK"}], ",", 
              RowBox[{"{", "}"}], ",", 
              RowBox[{"{", "\[IndentingNewLine]", 
               RowBox[{"(*", 
                RowBox[{"plaquette", ":"}], "*)"}], 
               RowBox[{
                RowBox[{
                 RowBox[{
                  FractionBox["p", 
                   RowBox[{
                   "M", "\[LeftDoubleBracket]", "a", 
                    "\[RightDoubleBracket]"}]], 
                  RowBox[{"(", 
                   RowBox[{
                    SubscriptBox["X", 
                    RowBox[{"0", ",", "a", ",", "b"}]], "-", 
                    SubscriptBox["X", 
                    RowBox[{"0", ",", "a", ",", "c"}]], "-", 
                    SubscriptBox["X", 
                    RowBox[{
                    RowBox[{
                    "Xs", "\[LeftDoubleBracket]", "c", 
                    "\[RightDoubleBracket]"}], ",", "a", ",", "b"}]], "+", 
                    SubscriptBox["X", 
                    RowBox[{
                    RowBox[{
                    "Xs", "\[LeftDoubleBracket]", "b", 
                    "\[RightDoubleBracket]"}], ",", "a", ",", "c"}]]}], 
                   ")"}]}], "+", 
                 RowBox[{
                  FractionBox[
                   RowBox[{
                    RowBox[{
                    "n", "\[LeftDoubleBracket]", "a", 
                    "\[RightDoubleBracket]"}], " ", "p"}], "\[CapitalNu]"], 
                  "\[Epsilon]", " ", 
                  SubscriptBox["X", 
                   RowBox[{"0", ",", "4", ",", 
                    RowBox[{"-", "a"}]}]]}]}], ",", "\[IndentingNewLine]", 
                RowBox[{"(*", 
                 RowBox[{"vertex", ":"}], "*)"}], 
                RowBox[{"(", 
                 RowBox[{
                  SubscriptBox["Z", 
                   RowBox[{"0", ",", "a", ",", "b"}]], "-", 
                  SubscriptBox["Z", 
                   RowBox[{
                    RowBox[{"-", 
                    RowBox[{
                    "Xs", "\[LeftDoubleBracket]", "b", 
                    "\[RightDoubleBracket]"}]}], ",", "a", ",", "b"}]], "+", 
                  SubscriptBox["Z", 
                   RowBox[{"0", ",", "a", ",", "c"}]], "-", 
                  SubscriptBox["Z", 
                   RowBox[{
                    RowBox[{"-", 
                    RowBox[{
